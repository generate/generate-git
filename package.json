{
  "name": "generate-git",
  "description": "Generator for initializing a git repository and adding first commit.",
  "version": "0.1.3",
  "homepage": "https://github.com/generate/generate-git",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "generate/generate-git",
  "bugs": {
    "url": "https://github.com/generate/generate-git/issues"
  },
  "license": "MIT",
  "files": [
